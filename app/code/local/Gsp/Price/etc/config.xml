<?xml version="1.0"?>
    <config>
        <modules>
            <Gsp_Price>
                <version>0.1.0</version>
            </Gsp_Price>
        </modules>
        <global>
            <models>
                <gspprice>
                    <class>Gsp_Repair_Model</class>
                </gspprice>
            </models>         
        </global>
        <frontend>       
            <routers>
                <gspprice>
                    <use>standard</use>
                    <args>
                        <module>Gsp_Price</module>
                        <frontName>price</frontName>
                    </args>
                </gspprice>
            </routers>
        </frontend>      
    </config>